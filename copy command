def display_build_user(abhise) {
 echo "Build username: ${abhise}"
 return 0
}
def display_node_name(node1){
 echo "Node name: ${node1}"
 return 0
}
pipeline {
 agent {label 'jenkinsnodes'}
 stages {
 stage("Env Build User"){
 steps{
 diplay_build_user(BUILD_USER)
 display_node_name(node1)
 }
 }
 stage("Env Node Name"){
 agent {label node1}
 steps{
 diplay_build_user(abhise)
 display_node_name(node1)
 }
 }
 }
}
